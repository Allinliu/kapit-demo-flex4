<?xml version="1.0" encoding="utf-8"?>
<s:Skin xmlns:fx="http://ns.adobe.com/mxml/2009"
		xmlns:s="library://ns.adobe.com/flex/spark"
		alpha.disabled="0.5" minHeight="71"
		>

    <fx:Metadata>[HostComponent("spark.components.SkinnableContainer")]</fx:Metadata> 
    
    <s:states>
        <s:State name="normal" />
        <s:State name="disabled" />
    </s:states>
    
	<s:Rect left="0" right="0" top="0" bottom="0">
		<s:fill>
			<s:LinearGradient rotation="90">
				<s:GradientEntry color="0xF9F9F9" ratio="0" />
				<s:GradientEntry color="0xF0F0F0" ratio="1" />
			</s:LinearGradient>
		</s:fill>
	</s:Rect>
    
    <s:Group id="contentGroup" minWidth="0" minHeight="0" horizontalCenter="0" verticalCenter="0">
        <s:layout>
            <s:HorizontalLayout gap="-2" horizontalAlign="center" verticalAlign="middle" />
        </s:layout>
    </s:Group>

</s:Skin>
